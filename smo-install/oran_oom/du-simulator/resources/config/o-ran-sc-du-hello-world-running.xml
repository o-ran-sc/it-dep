<network-function xmlns="urn:o-ran-sc:yang:o-ran-sc-du-hello-world">
{{ range $ruSim := .Values.ruSimulators }}

    <du-to-ru-connection>
      <name>{{ $ruSim.name }}</name>
      <administrative-state>{{ $ruSim.administrativeState }}</administrative-state>
    </du-to-ru-connection>
{{ end }}

</network-function>
